<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Lieux" table="lieux">
    <indexes>
      <index name="lieux_villes_fk" columns="villes_no_ville"/>
    </indexes>
    <id name="noLieu" type="integer" column="no_lieu">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomLieu" type="string" column="nom_lieu" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rue" type="string" column="rue" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="latitude" type="float" column="latitude" precision="10" scale="0" nullable="true"/>
    <field name="longitude" type="float" column="longitude" precision="10" scale="0" nullable="true"/>
    <field name="villesNoVille" type="integer" column="villes_no_ville" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
